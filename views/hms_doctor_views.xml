<odoo>
    <record id="view_hms_doctor_form" model="ir.ui.view">
        <field name="name">hms.doctor.form</field>
        <field name="model">hms.doctor</field>
        <field name="arch" type="xml">
            <form string="Doctor">
                <sheet>
                    <group>
                        <field name="first_name"/>
                        <field name="last_name"/>
                        <field name="image"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_hms_doctor_tree" model="ir.ui.view">
        <field name="name">hms.doctor.tree</field>
        <field name="model">hms.doctor</field>
        <field name="arch" type="xml">
            <tree string="Doctors">
                <field name="first_name"/>
                <field name="last_name"/>
                <field name="image"/>
            </tree>
        </field>
    </record>

    <record id="action_hms_doctor" model="ir.actions.act_window">
        <field name="name">Doctors</field>
        <field name="res_model">hms.doctor</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_hms_doctors"
              name="Doctors" parent="menu_hms_root"
              action="action_hms_doctor" sequence="30"/>
</odoo>
